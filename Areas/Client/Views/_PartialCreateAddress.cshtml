@model WebApplication2.Models.Address

<h4>Address</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="ClientWebId" class="control-label"></label>
            <select asp-for="ClientWebId" class="form-control" asp-items="ViewBag.ClientWebId"></select>
        </div>
        <div class="form-group">
            <label asp-for="Street" class="control-label"></label>
            <input asp-for="Street" class="form-control" />
            <span asp-validation-for="Street" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Building" class="control-label"></label>
            <input asp-for="Building" class="form-control" />
            <span asp-validation-for="Building" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Deck" class="control-label"></label>
            <input asp-for="Deck" class="form-control" />
            <span asp-validation-for="Deck" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Letter" class="control-label"></label>
            <input asp-for="Letter" class="form-control" />
            <span asp-validation-for="Letter" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="CountryId" class="control-label"></label>
            @Html.DropDownList("countrys", ViewData["countrys"] as List<SelectListItem>, "Seleccione --", new { @class = "form-control  select2 select2-hidden-accessible" })
            <span asp-validation-for="CountryId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="ProvinceId" class="control-label"></label>
            @Html.DropDownList("provinces", ViewData["provinces"] as List<SelectListItem>, "Seleccione --", new { @class = "form-control  select2 select2-hidden-accessible" })
            <span asp-validation-for="ProvinceId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="LocalityId" class="control-label"></label>
            @Html.DropDownList("localities", ViewData["localities"] as List<SelectListItem>, "Seleccione --", new { @class = "form-control  select2 select2-hidden-accessible" })
            <span asp-validation-for="LocalityId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="ZipCodeId" class="control-label"></label>
            <input asp-for="ZipCodeId" class="form-control" />
            <span asp-validation-for="ZipCodeId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </div>
</div>

